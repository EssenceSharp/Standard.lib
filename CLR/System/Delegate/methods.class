	"Class methods of System.Delegate"
	
	protocol: #'multicast operators' method:
	
	[## combine: delegate1 with: delegate2
	
		<invokeMethod: #Combine>
	];
	
	protocol: #'multicast operators' method:
	
	[## removeFrom: aDelegate invocationListOf: removal
		"Answers a new delegate constructed by removing the invocation
		list of <removal> from that of <aDelegate>"
		
		<invokeMethod: #Remove>
	]