	"Instance methods for AnnouncementSubscription"	protocol: #accessing method:		[## action				^action	];	protocol: #accessing method:		[## action: anObject				action := anObject	];	protocol: #accessing method:		[## announcementClass				^announcementClass	];	protocol: #accessing method:		[## announcementClass: anObject				announcementClass := anObject	];	protocol: #accessing method:		[## announcer				^announcer	];	protocol: #accessing method:		[## announcer: anAnnouncer			announcer := anAnnouncer	];		protocol: #accessing method:		[## subscriber			^subscriber	];	protocol: #accessing method:		[## subscriber: aSubscriber			subscriber := aSubscriber	];	protocol: #testing method:		[## handles: anAnnouncement				^announcementClass handles: anAnnouncement	]